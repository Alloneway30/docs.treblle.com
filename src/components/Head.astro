---
import type { Props } from "@astrojs/starlight/props";
import Default from "@astrojs/starlight/components/Head.astro"
---

<Default {...Astro.props}><slot /></Default>


{Astro.props.entry.data.image ? <meta property="og:image" content={new URL(Astro.props.entry.data.image, Astro.site)} /> : <meta property="og:image" content={new URL("default-og-image.png", Astro.site)} /> }

<link rel="sitemap" href="/sitemap-index.xml" />